{
  "name": "web1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:all": "concurrently --kill-others \"npm run dev\" \"npm run css\"",
    "dev": "RUNTIME=develop next dev -p 1203",
    "dev:prod": "RUNTIME=prod next dev -p 1203",
    "css": "tailwindcss -i ./css/main.css -o ./public/css/default.css --watch",
    "test": "cypress open",
    "build": "next build",
    "start": "RUNTIME=prod next start -p 1203",
    "lint": "next lint"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.11.1",
    "@sentry/react": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "concurrently": "^7.3.0",
    "next": "12.2.5",
    "postcss": "^8.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "axios": "^0.27.2",
    "cypress": "^10.7.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5"
  }
}
