    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "app-logger": "file:../../library/app-logger",
    "axios": "^0.27.2",
    "concurrently": "^7.4.0",
    "express": "^4.18.1",
    "framer-motion": "^7.3.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "pm2": "^5.2.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
